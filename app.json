{
  "expo": {
    "name": "Aniteka",
    "slug": "aniteka",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "aniteka",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#080808"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.osu-tenef.aniteka",
      "associatedDomains": [
        "applinks:hikka.io"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#080808"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.osu_tenef.aniteka",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "aniteka"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.1",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-web-browser",
        {
          "android": {
            "package": "com.android.chrome"
          }
        }
      ],
      "expo-asset",
      "expo-secure-store",
      "expo-image-picker",
      "expo-font"
    ],
    "owner": "osu_tenef",
    "extra": {
      "eas": {
        "projectId": "b10e7c03-5c1f-48fc-91be-e38230a6a9f1"
      }
    }
  }
}
